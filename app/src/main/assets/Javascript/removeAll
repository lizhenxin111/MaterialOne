javascript:function removeAll(){
    var url = document.getElementsByTagName("source")[0].getAttribute("src");
    var title = document.querySelector("div.text-title").innerText;
    var info = document.querySelector("div.text-music-info").getElementsByTagName("div")[0].innerText;
    window.music.showButton(url, title, info);
    var a = document.querySelector("div.header");
    a.parentNode.removeChild(a);
    var c = document.querySelector("div.footer");
    c.parentNode.removeChild(c);
    var d = document.querySelector("p.text-title");
    d.parentNode.removeChild(d);
    var e = document.querySelector("hr.sort-separate-line15");
    e.parentNode.removeChild(e);
    var g = document.querySelector("div.text-detail");
    g.onclick = '';
}